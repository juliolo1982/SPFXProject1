{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx-build/config.2.0.schema.json",
  "version": "2.0",
  "bundles": {
    "hola-pnp-web-part": {
      "components": [
        {
          "entrypoint": "./lib/webparts/holaPnp/HolaPnpWebPart.js",
          "manifest": "./src/webparts/holaPnp/HolaPnpWebPart.manifest.json"
        }
      ]
    }
  },
  "externals": {
    "jquery": {
      "path": "node_modules/jquery/dist/jquery.slim.min.js",
      "globalName": "jQuery"
    },
    "bootstrap": {
      "path": "node_modules/bootstrap/dist/js/bootstrap.bundle.min.js",
      "globalName": "bootstrap"
    }
  },
  "localizedResources": {
    "HolaPnpWebPartStrings": "lib/webparts/holaPnp/loc/{locale}.js",
    "PropertyControlStrings": "node_modules/@pnp/spfx-property-controls/lib/loc/{locale}.js"
  }
}